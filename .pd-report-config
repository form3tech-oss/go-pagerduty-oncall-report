# Explicitly set report time range (RFC822)
reportTimeRange:
  start: 01 Dec 22 00:00 AEDT
  end: 20 Mar 23 00:00 AEDT

# Rotation general information
rotationInfo:
  dailyRotationStartsAt: 0
  checkRotationChangeEvery: 30 # minutes

defaultUserTimezone: Australia/Melbourne # default user timezone for users that are in the report but their account has been excluded from PagerDuty

defaultHolidayCalendar: au_vic # default calendar to use for users not specified in config, allows you to only define users with different calendars. If value not specified then fall back to old behaviour

# Rotation excluded hours by day type
rotationExcludedHours:
  - day: weekday
    excludedStartsAt: 9
    excludedEndsAt: 17

# Rotation prices by day type
rotationPrices:
  currency: $
  daysInfo:
    - day: weekday
      price: 1
    - day: weekend
      price: 1
    - day: bankholiday
      price: 1

